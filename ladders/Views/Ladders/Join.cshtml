@model ladders.Models.LadderModel


@{
    ViewData["Title"] = "Join";
}

<h2>Join Ladder @(Model.Name)</h2>

<h4>Do you want to join this ladder?</h4>

<form asp-action="Join">

    <input type="hidden" asp-for="Id" />
    <div class="form-group">
        <input type="submit" value="Join" class="btn btn-default" />
    </div>
</form>